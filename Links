pip install qrcode[pil]
import qrcode

# Define los tres enlaces que deseas convertir en códigos QR
links = [
    "https://viep.com.mx/edehaci-m/",
    "https://correobuap-my.sharepoint.com/:f:/g/personal/jose_vegag_alumno_buap_mx/EpW6SeN_bpVCrL8aHrGMzHMBBEknWfwgHNl1AEQ9VSPayA?e=GgH8ag",
    "https://forms.gle/nBV36dyjiciN7VN56"
]

# Crea los códigos QR y guárdalos en archivos PNG
for i, link in enumerate(links):
    qr = qrcode.QRCode(
        version=1,
        error_correction=qrcode.constants.ERROR_CORRECT_L,
        box_size=10,
        border=4,
    )
    qr.add_data(link)
    qr.make(fit=True)

    img = qr.make_image(fill_color="black", back_color="white")
    img.save(f"qr_code_{i+1}.png")
    print(f"Creado qr_code_{i+1}.png para {link}")
